		<tls:key-store type="pkcs12" path="certs/check-in-papi-dev.p12" alias="server" keyPassword="mule12345" password="mule12345"/>
	</tls:context>
	    
    <apikit:config name="apiConfig" api="resource::cd8b296d-8848-4a91-98ed-4a47f73703bd:check-in-papi:1.0.0:raml:zip:api.raml" outboundHeadersMapName="outboundHeaders" httpStatusVarName="httpStatus" />

	
	<api-gateway:autodiscovery apiId="16734869" ignoreBasePath="true" doc:name="apiAutodiscovery" doc:id="f0e8f222-11f8-4c88-a5cb-961185b9831e" flowRef="api-main" />	
	

</mule>
